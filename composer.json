{
    "name": "opsway/opsdesk",
    "description": "Tocat CMF based on ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "tocat",
        "application",
        "doctrine",
        "CRM"
    ],
    "homepage": "https://github.com/opsway/tocat",
    "authors": [
        {
            "name": "Alexandr Vronskiy",
            "email": "shandy.developer@gmail.com"
        }
    ],
    "config": {
        "process-timeout": 5000
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/opsway/ZfcUserOnelogin"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-intl": "*",
        "zendframework/zendframework": "~2.4",
        "zfcampus/zf-apigility": "~1.0",
        "kbsali/redmine-api":"~1.0",
        "zf-commons/zfc-user-doctrine-orm": "1.*",
        "opsway/zfc-user-onelogin": "dev-master",
        "danielss89/zfc-user-admin": "dev-master",
        "bjyoungblood/bjy-authorize": "~1.4",
        "gedmo/doctrine-extensions": "2.3.*",
        "shandy-dev/humus-doctrine-hydrator": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=3.7,<4",
        "squizlabs/php_codesniffer" : "1.4.*",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zftool": "dev-master",
        "zfcampus/zf-apigility-admin": "~1.0-dev@dev",
        "zfcampus/zf-development-mode": "~2.0",
        "zfcampus/zf-apigility-documentation": "~1.0",
        "zendframework/zend-developer-tools": "dev-master",
        "zfcampus/zf-deploy": "~1.0",
        "zfcampus/zf-apigility-welcome": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "OpsWay\\TocatCore\\": "modules/OpsWay/TocatCore/code/",
            "OpsWay\\TocatUser\\": "modules/OpsWay/TocatUser/code/",
            "OpsWay\\TocatBudget\\": "modules/OpsWay/TocatBudget/code/",
            "OpsWay\\AppManager\\": "modules/OpsWay/AppManager/code/"
        }
    }
}
